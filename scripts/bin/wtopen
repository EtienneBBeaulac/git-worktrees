#!/usr/bin/env zsh
# Wrapper to run wtopen as a CLI command
# Sources the sibling ../wtopen function file and invokes wtopen

script_dir="${0:A:h}"
script_root="${script_dir}/.."

if [[ -f "${script_root}/wtopen" ]]; then
  source "${script_root}/wtopen"
else
  echo "Error: wtopen not found at ${script_root}/wtopen" >&2
  exit 2
fi

wtopen "$@"
